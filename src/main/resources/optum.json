        "timeout": 10,
        "asyncwait": false
      },
      "f": {
        "operation": "jsclick",
        "selector": "xpath",
        "name": "//{variable}",
        "variable": true
      },
      "g": {
        "operation": "wait",
        "selector": "xpath",
        "name": "//*[contains(@class, 'data-table')]/tr[1]/td[2][string-length(text()) > 0]",
        "timeout": 10,
        "asyncwait": false
      },
      "h": {
        "operation": "snapshot"
      }
    }
  }
