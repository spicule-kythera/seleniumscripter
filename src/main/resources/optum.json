{
  "a": {
    "operation": "loadpage"
  },
  "b": {
    "operation": "keys",
    "selector": "xpath",
    "name": "//*[@id=\"drugNameInput\"]",
    "value": "abilify",
    "searchfix": true,
    "postDelay": 50,
    "delay": 500
  },
  "c": {
    "operation": "captureList",
    "selector": "xpath",
    "name": "//ul[contains(@class,\"dropdown-menu\")]//a",
    "type": "text",
    "variable": "searchterms"
  },
  "d": {
    "operation": "parallel",
    "type": "variable",
    "variable": "searchterms",
    "subscript": "searchitem",
    "parallelizm": 3
  },
  "subscripts": {
    "searchitem": {
      "a": {
        "operation": "loadpage"
      },
      "b": {
        "operation": "keys",
        "selector": "xpath",
        "name": "//*[@id=\"drugNameInput\"]",
        "value": "${loopvalue}",
        "searchfix": true,
        "postDelay": 50,
        "delay": 500
      },
      "c": {
        "operation": "click",
        "selector": "xpath",
        "name": "//ul[contains(@class,\"dropdown-menu\")]//a[1]"
      },
      "d": {
        "operation": "wait",
        "selector": "xpath",
        "name": "//*[contains(@class, 'btn') and text() = 'Coverage details']",
        "timeout": 20
      },
      "e": {
        "operation": "snapshot"
      },
      "f": {
        "operation": "captureList",
        "selector": "xpath",
        "name": "//a[@class=\"btn no-link ng-binding\"]",
        "type": "xpath",
        "variable": "dosagelabels"
      },
      "g": {
        "operation": "loop",
        "type": "variable",
        "variable": "dosagelabels",
        "subscript": "doseSubScript"
      },
      "subscripts": {
        "doseSubScript": {
          "1-wait": {
            "operation": "wait",
            "selector": "xpath",
            "name": "//*[contains(@class, 'btn') and text() = 'Coverage details']",
            "timeout": 20
          },
          "2-click": {
            "operation": "jsclick",
            "selector": "xpath",
            "name": "//{variable}",
            "variable": true
          },
          "3-wait": {
            "operation": "wait",
            "selector": "xpath",
            "name": "//*[contains(@class, 'data-table')]/tr[1]/td[2][string-length(text()) > 0]",
            "timeout": 20
          },
          "4-subscript": {
            "operation": "snapshot"
          },
          "5-backup": {
            "operation": "click",
            "selector": "xpath",
            "name": "//div/p/a[@data-ng-click='backToMedicationCabinet()']"
          }
        }
      }
    }
  }
}